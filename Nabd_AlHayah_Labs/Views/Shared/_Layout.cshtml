	<script>
		document.addEventListener('DOMContentLoaded', () => {
			const toasts = document.querySelectorAll('.toast-msg');

			toasts.forEach((toast, index) => {
				// تأخير ظهور كل toast
				setTimeout(() => {
					toast.classList.add('show');
				}, index * 150);

				// إعداد الإغلاق التلقائي
				const autoRemove = setTimeout(() => {
					hideToast(toast);
				}, 4000);

				// زر الإغلاق
				const closeBtn = toast.querySelector('.toast-close');
				closeBtn.addEventListener('click', () => {
					clearTimeout(autoRemove);
					hideToast(toast);
				});

				// إغلاق عند النقر على الـ toast نفسه
				toast.addEventListener('click', (e) => {
					if (e.target === toast) {
						clearTimeout(autoRemove);
						hideToast(toast);
					}
				});
			});

			function hideToast(toast) {
				toast.classList.remove('show');
				toast.classList.add('hiding');

				setTimeout(() => {
					toast.remove();

					// إعادة ترتيب الـ toasts المتبقية
					const remainingToasts = document.querySelectorAll('.toast-msg');
					if (remainingToasts.length === 0) {
						document.getElementById('toastContainer').remove();
					}
				}, 600);
			}

			// إغلاق جميع الـ toasts عند الضغط على Esc
			document.addEventListener('keydown', (e) => {
				if (e.key === 'Escape') {
					toasts.forEach(toast => {
						hideToast(toast);
					});
				}
			});
		});
	</script>
